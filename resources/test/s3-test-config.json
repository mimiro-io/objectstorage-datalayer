{
    "id": "test-local",
    "storageBackends": [
        {
            "dataset": "s3-athena",
            "storageType": "S3",
            "athenaCompatible": true,
            "storeDeleted": false,
            "stripProps": false,
            "decode": {
                "namespaces": {
                    "a": "http://people/base",
                    "b": "http://places/base"
                }
            },
            "props": {
                "resourceName": "latest.json",
                "bucket": "s3-test-bucket",
                "endpoint": "http://localhost:8888",
                "region": "us-east-1",
                "key": "AccessKeyId",
                "secret": "S3_STORAGE_SECRET_ACCESSKEYID"
            }
        },
        {
            "dataset": "s3-athena-stripped",
            "storageType": "S3",
            "athenaCompatible": true,
            "storeDeleted": false,
            "stripProps": true,
            "decode": {
                "namespaces": {
                    "a": "http://people/base",
                    "b": "http://places/base"
                },
                "propertyPrefixes": {
                    "name": "a:a",
                    "firstname": "a",
                    "surname": "a",
                    "age": "a",
                    "vaccinated": "a",
                    "address": "b"
                },
                "idProperty": "id",
                "refs": ["address"]
            },
            "props": {
                "resourceName": "latest.json",
                "bucket": "s3-test-bucket",
                "endpoint": "http://localhost:8888",
                "region": "us-east-1",
                "key": "AccessKeyId",
                "secret": "S3_STORAGE_SECRET_ACCESSKEYID"
            }
        },
        {
            "dataset": "s3-athena-deletedTrue",
            "storageType": "S3",
            "athenaCompatible": true,
            "storeDeleted": true,
            "stripProps": false,
            "props": {
                "resourceName": "latest.json",
                "bucket": "s3-test-bucket",
                "endpoint": "http://localhost:8888",
                "region": "us-east-1",
                "key": "AccessKeyId",
                "secret": "S3_STORAGE_SECRET_ACCESSKEYID"
            }
        },
        {
            "dataset": "s3-csv-mapping",
            "storageType": "S3",
            "storeDeleted": false,
            "stripProps": false,
            "csv": {
                "header": true,
                "encoding": "UTF-8",
                "separator": ",",
                "order": [
                    "id",
                    "firstname",
                    "surname",
                    "age",
                    "vaccinated"
                ]
            },
            "props": {
                "bucket": "s3-test-bucket",
                "resourceName": "test.csv",
                "endpoint": "http://localhost:8888",
                "region": "us-east-1",
                "key": "AccessKeyId",
                "secret": "S3_STORAGE_SECRET_ACCESSKEYID"
            }
        },
        {
            "dataset": "s3-parquet-mapping",
            "storageType": "S3",
            "storeDeleted": false,
            "stripProps": true,
            "parquet": {
                "flushThreshold": 20,
                "schema": "message test_schema { required binary id (STRING); optional binary firstname (STRING); }"
            },
            "props": {
                "bucket": "s3-test-bucket",
                "resourceName": "test.parquet",
                "endpoint": "http://localhost:8888",
                "region": "us-east-1",
                "key": "AccessKeyId",
                "secret": "S3_STORAGE_SECRET_ACCESSKEYID"
            }
        },
        {
            "dataset": "s3-flatfile",
            "storageType": "S3",
            "storeDeleted": false,
            "props": {
                "bucket": "s3-test-bucket",
                "resourceName": "",
                "customResourcePath": true,
                "endpoint": "http://localhost:8888",
                "region": "us-east-1",
                "key": "AccessKeyId",
                "secret": "S3_STORAGE_SECRET_ACCESSKEYID"
            },
            "flatFile": {
                "fields": {
                    "foo": {
                        "substring":[[0,2]]
                    },
                    "bar": {
                        "substring":[[2,5]],
                        "type":"integer"
                    }
                }
            },
            "decode": {
                "defaultNamespace":"_",
                "namespaces": {
                    "_":"http://example.io/foo/"
                },
                "propertyPrefixes":{},
                "refs":[],
                "idProperty":"foo"
            }
        },
        {
            "dataset": "s3-parquet-test",
            "storageType": "S3",
            "stripProps": true,
            "parquet": {
                "flushThreshold": 20,
                "schema": "message test_schema { required int64 age; optional binary id (STRING);}"
            },
            "decode": {
                "defaultNamespace": "_",
                "namespaces": {
                    "_": "http://data.io/foo/bar/"
                },
                "propertyPrefixes":{
                    "age": "_:_"
                },
                "idProperty": "age"
            },
            "props": {
                "bucket": "s3-test-bucket",
                "endpoint": "http://localhost:8888",
                "region": "us-east-1",
                "key": "AccessKeyId",
                "secret": "S3_STORAGE_SECRET_ACCESSKEYID"
            }
        }
    ]
}
